                <div class="carousel-item active">
                    @{
                        DataTable filaTop1 = ViewData["TopParte1"] as DataTable;
                        int Cont = 0;
                        foreach (DataRow row in filaTop1.Rows)
                        {
                            string Foto = "data:image / jpeg; base64," + Convert.ToBase64String((byte[])row["Imagen"]);

                            if (Cont == 0)
                            {
                                <div class="col-md-4">
                                    <div class="card md-2">
                                        <img class="img-fluid" src="@Foto" />
                                        <div class="card-body">
                                            <h4 class="card-title">@row["Nombre"]</h4>
                                            <p class="card-text">Texto prueba</p>
                                        </div>
                                    </div>
                                </div>
                            }
                            else
                            {
                                <div class="col-md-4 clearfix d-none d-md-block">
                                    <div class="card md-2">
                                        <img class="img-fluid" src="@Foto" />
                                        <div class="card-body">
                                            <h4 class="card-title">@row["Nombre"]</h4>
                                            <p class="card-text">Texto prueba</p>
                                        </div>
                                    </div>
                                </div>
                            }
                            Cont++;
                        }
                    }
                </div>
                <!--/.First slide-->
                <!--Second slide-->
                <div class="carousel-item">
                    @{
                        DataTable filaTop2 = ViewData["TopParte2"] as DataTable;
                        int ContP2 = 0;
                        foreach (DataRow row in filaTop2.Rows)
