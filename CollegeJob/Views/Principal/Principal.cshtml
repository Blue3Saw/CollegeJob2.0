<body>
    <nav class="mb-1 navbar navbar-expand-lg navbar-dark fixed-top scrolling-navbar" id="navbar">
        <a class="navbar-brand" href="#"><strong>CollegeJob</strong></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarContent" aria-controls="navbarContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
        <div class="collapse navbar-collapse" id="navbarContent" style="">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link waves-effect waves-light" href="#">¿Quiénes somos?</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link waves-effect waves-light" href="#">Misión</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link waves-effect waves-light" href="#">Visión</a>
                </li>
            </ul>

            <ul class="navbar-nav ml-auto">
                <li class="nav-item"><a class="nav-link" data-toggle="modal" data-target="#modalRegistroForm">Registro</a></li>
                <li class="nav-item"><a class="nav-link" data-toggle="modal" data-target="#modalContactForm">Inicio sesión</a></li>
            </ul>
        </div>
    </nav>



    <div class="modal fade" id="modalContactForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <!--Modal: Contact form-->
        <div class="modal-dialog cascading-modal" role="document">

            <!--Content-->
            <div class="modal-content">

                <!--Header-->
                <div class="modal-header default-color-dark white-text">
                    <h4 class="title">
                        <i class="fa fa-user"></i> Inicio de sesión
                    </h4>
                    <button type="button" class="close waves-effect waves-light" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <!--Body-->
                <div class="modal-body">
                    @using (Ajax.BeginForm("Login", "Principal", new AjaxOptions() { HttpMethod = "POST" }))
                    {
                        @Html.AntiForgeryToken()
                        <!-- Material input name -->
                        <div class="md-form form-sm">
                            <i class="fa fa-envelope prefix"></i>
                            <input type="text" id="materialFormNameModalEx1" name="Email" class="form-control form-control-sm" required="required">
                            <label for="materialFormNameModalEx1">Correo electrónico</label>
                        </div>

                        <!-- Material input email -->
                        <div class="md-form form-sm">
                            <i class="fa fa-lock prefix"></i>
                            <input type="password" id="materialFormEmailModalEx1" name="Contraseña" class="form-control form-control-sm" required="required">
                            <label for="materialFormEmailModalEx1">Contraseña</label>
                        </div>

                        <div class="text-center mt-4 mb-2">
                            <button class="btn default-color-dark">
                                Iniciar
                                <i class="fa fa-send ml-2"></i>
                            </button>
                        </div>
                    }

                </div>
            </div>
            <!--/.Content-->
        </div>
        <!--/Modal: Contact form-->
    </div>

    <div class="modal fade" id="modalRegistroForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <!--Modal: Contact form-->
        <div class="modal-dialog cascading-modal" role="document">

            <!--Content-->
            <div class="modal-content">

                <!--Header-->
                <div class="modal-header default-color-dark white-text">
                    <h4 class="title">
                        <i class="fa fa-user"></i> Registro
                    </h4>
                    <button type="button" class="close waves-effect waves-light" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <!--Body-->
                <div class="modal-body">

                    <div class="md-form form-sm">
                        <i class="fa fa-user prefix"></i>
                        <input type="text" id="materialFormNameModalNombre" class="form-control form-control-sm">
                        <label for="materialFormNameModalNombre">Nombre(s)</label>
                    </div>

                    <div class="md-form form-sm">
                        <i class="fa fa-user prefix"></i>
                        <input type="text" id="materialFormNameModalNombre" class="form-control form-control-sm">
                        <label for="materialFormNameModalNombre">Apellidos</label>
                    </div>

                    <div class="md-form form-sm">
                        <i class="fa fa-calendar prefix"></i>
                        <input type="date" id="materialFormNameModalNombre" class="form-control form-control-sm" data-toggle="tooltip" data-placement="top" title="Fecha de nacimiento">
                    </div>

                    <div class="md-form form-sm">
                        <i class="fa fa-phone prefix"></i>
                        <input type="tel" id="materialFormNameModalNombre" class="form-control form-control-sm">
                        <label for="materialFormNameModalNombre">Teléfono</label>
                    </div>

                    <!-- Material input name -->
                    <div class="md-form form-sm">
                        <i class="fa fa-envelope-o prefix"></i>
                        <input type="text" id="materialFormNameModalEx1" class="form-control form-control-sm">
                        <label for="materialFormNameModalEx1">Correo electrónico</label>
                    </div>

                    <!-- Material input email -->
                    <div class="md-form form-sm">
                        <i class="fa fa-lock prefix"></i>
                        <input type="password" id="materialFormEmailModalEx1" class="form-control form-control-sm">
                        <label for="materialFormEmailModalEx1">Contraseña</label>
                    </div>

                    <div class="text-center mt-4 mb-2">
                        <button class="btn default-color-dark">
                            Iniciar
                            <i class="fa fa-send ml-2"></i>
                        </button>
                    </div>

                </div>
            </div>
            <!--/.Content-->
        </div>
        <!--/Modal: Contact form-->
    </div>

